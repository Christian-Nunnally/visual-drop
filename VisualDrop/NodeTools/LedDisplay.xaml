<UserControl x:Class="ColorOrgan5Nodes.NodeTools.LedDisplay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d" 
             d:DesignHeight="152" d:DesignWidth="152">
    <UserControl.Resources>
        <Style TargetType="Rectangle">
            <Setter Property="Margin" Value="1"/>
        </Style>
    </UserControl.Resources>
    <Border Background="#FFFF" BorderBrush="#f222" BorderThickness="1" RenderTransformOrigin="0.5, 0.5">
        <Border.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform Angle="-90"/>
                <TranslateTransform/>
            </TransformGroup>
        </Border.RenderTransform>
        <Grid MouseMove="LedDisplay_OnMouseMove">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            
            <Rectangle Name="Pixel1" Fill="#FFF4F4F5" Stroke="#F222" MouseDown="PixelMouseDown"/>
            <Rectangle Name="Pixel2" Fill="#FFF4F4F5" Stroke="#F222" Grid.Column="1" MouseDown="PixelMouseDown"/>
            <Rectangle Name="Pixel3" Fill="#FFF4F4F5" Stroke="#F222" Grid.Column="2" MouseDown="PixelMouseDown"/>
            <Rectangle Name="Pixel4" Fill="#FFF4F4F5" Stroke="#F222" Grid.Column="3" MouseDown="PixelMouseDown"/>
            <Rectangle Name="Pixel5" Fill="#FFF4F4F5" Stroke="#F222" Grid.Column="4" MouseDown="PixelMouseDown"/>
            <Rectangle Name="Pixel6" Fill="#FFF4F4F5" Stroke="#F222" Grid.Column="5" MouseDown="PixelMouseDown"/>
            <Rectangle Name="Pixel7" Fill="#FFF4F4F5" Stroke="#F222" Grid.Column="6" MouseDown="PixelMouseDown"/>
            <Rectangle Name="Pixel8" Fill="#FFF4F4F5" Stroke="#F222" Grid.Column="7" MouseDown="PixelMouseDown"/>

            <Rectangle Name="Pixel9" Fill="#FFF4F4F5" Stroke="#F222" Grid.Row="1" MouseDown="PixelMouseDown"/>
            <Rectangle Name="Pixel10" Fill="#FFF4F4F5" Stroke="#F222" Grid.Row="1" Grid.Column="1" MouseDown="PixelMouseDown"/>
            <Rectangle Name="Pixel11" Fill="#FFF4F4F5" Stroke="#F222" Grid.Row="1" Grid.Column="2" MouseDown="PixelMouseDown"/>
            <Rectangle Name="Pixel12" Fill="#FFF4F4F5" Stroke="#F222" Grid.Row="1" Grid.Column="3" MouseDown="PixelMouseDown"/>
